  },
  "devDependencies": {
    "@nativescript/android": "8.9.2",
    "@nativescript/webpack": "~5.0.0",
    "dts-bundle-generator": "^9.5.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.7"
  }
}
